 {\r\n    let timer = undefined;\r\n    let remainingTime = gameDuration;\r\n    updateTimerText(gameDuration);\r\n    timer = setInterval(() => {\r\n        if (remainingTime <= 0) {\r\n            clearInterval(timer);\r\n            finishGame(false);\r\n\r\n            function finishGame(result) {\r\n                showPopupWidthText(result ? 'YOU WIN!' : 'YOU LOSE!');\r\n                hideTimerAndCount();\r\n            }\r\n\r\n            function showPopupWidthText(text) {\r\n                const popUp = document.querySelector('.pop-up');\r\n                const popUpMessage = document.querySelector('.pop-up__message');\r\n                popUp.style.visibility = 'visible';\r\n                popUpMessage.innerText = text;\r\n            }\r\n\r\n            function hideTimerAndCount() {\r\n                const gameTimer = document.querySelector('.header__time');\r\n                const gameCount = document.querySelector('.header__count');\r\n                gameTimer.style.visibility = 'hidden';\r\n                gameCount.style.visibility = 'hidden';\r\n            }\r\n        }\r\n        updateTimerText(--remainingTime);\r\n    }, 1000);\r\n}\r\n\r\nfunction updateTimerText(time) {\r\n    const minutes = Math.floor(time / 60);\r\n    const seconds = time % 60;\r\n    gameTimer.innerText = `${minutes}:${seconds}`\r\n}"},"lineCount":null}},"error":null,"hash":"b58c7a672fd495148e9b5111d138e19b","cacheData":{"env":{}}}