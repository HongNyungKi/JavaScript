\r\n    opacity: 0.8;\r\n    border-radius: 0 0 20px 20px;\r\n    box-shadow: 4px 10px 22px -5px white;\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .game__header {\r\n      display: flex;\r\n      flex-direction: column;\r\n      justify-content: center;\r\n      align-items: center;\r\n\r\n      .header__btn-play {\r\n        margin-top: $header-margin * 2;\r\n        width: 75px;\r\n        height: 75px;\r\n        background: #fca311;\r\n        font-size: 30px;\r\n        border: 5px solid black;\r\n        border-radius: 5px;\r\n        transition: all 80ms ease-in;\r\n\r\n        &:hover {\r\n          transform: scale(1.1);\r\n        }\r\n      }\r\n\r\n      .header__btn-stop {\r\n        margin-top: $header-margin * 2;\r\n        width: 75px;\r\n        height: 75px;\r\n        background: #fca311;\r\n        font-size: 30px;\r\n        border: 5px solid black;\r\n        border-radius: 5px;\r\n        transition: all 80ms ease-in;\r\n\r\n        &:hover {\r\n          transform: scale(1.1);\r\n        }\r\n\r\n        display: none;\r\n      }\r\n\r\n      .header__time {\r\n        margin-top: $header-margin;\r\n        width: 120px;\r\n        height: 50px;\r\n        background: #e9ecef;\r\n        border: 3px solid black;\r\n        border-radius: 10px;\r\n        font-size: 40px;\r\n        text-align: center;\r\n        visibility: hidden;\r\n      }\r\n\r\n      .header__count {\r\n        margin-top: $header-margin;\r\n        width: 65px;\r\n        height: 65px;\r\n        background: #f71735;\r\n        color: #41ead4;\r\n        font-size: 50px;\r\n        font-weight: bold;\r\n        text-align: center;\r\n        border: 3px solid black;\r\n        border-radius: 50%;\r\n        visibility: hidden;\r\n      }\r\n    }\r\n\r\n    .game__field {\r\n      width: 100%;\r\n      height: 100%;\r\n      position: relative;\r\n\r\n    }\r\n\r\n    .pop-up {\r\n      width: 600px;\r\n      height: 350px;\r\n      position: absolute;\r\n      bottom: 10%;\r\n      left: 50%;\r\n      transform: translate(-50%);\r\n      visibility: hidden;\r\n\r\n      .pop-up__wrap {\r\n        position: absolute;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%, -50%);\r\n        display: flex;\r\n        flex-direction: column;\r\n\r\n        .pop-up__refresh {\r\n          background: wheat;\r\n          width: 100px;\r\n          height: 100px;\r\n          border: 2px solid black;\r\n          border-radius: 50%;\r\n          font-size: 40px;\r\n          margin: auto;\r\n          transition: all 100ms ease-in;\r\n\r\n          &:hover {\r\n            transform: scale(1.1) rotate(45deg);\r\n          }\r\n        }\r\n\r\n        .pop-up__message {\r\n          margin-top: $header-margin;\r\n          color: white;\r\n          font-size: 30px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"4d71e54c8c1e13d542e321d4fc63cf2c","cacheData":{"env":{}}}